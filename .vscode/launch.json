    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: FastAPI Test",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-vv",
                "--durations=50",
                "specs/integration/test_client.py::TestTask::test_should_not_add_task_when_doesnt_exist_client"
            ],
            "env": {
                "FASTAPI_ENV": "testing"
            }
        }
    ]
}